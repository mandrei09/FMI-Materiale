@model modelExamen.Models.Giftcard

<div class="card-body">
    <h3 class="card-title alert-success py-3 px-3 rounded2">@Model.Denumire</h3>
    <div class="card-text">@Model.Descriere</div>
    <div class="d-flex flex-column">
        <div>
            @Model.Brand.Nume
        </div>
        <div>
            @Model.Procent
        </div>
        <span class="alert-success" style="width:fit-content">@Model.DataExp</span>
    </div>
</div>
